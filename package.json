{
  "author": "Triply <info@triply.cc>",
  "dependencies": {
    "@triplyetl/etl": "^2.0.6"
  },
  "description": "ETL for mcal",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.195",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.18.35",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^26.9.0",
    "husky": "8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn run util:lint:precommit"
    ]
  },
  "main": "lib/main.js",
  "name": "mcal",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --preserveWatchOutput",
    "postversion": "versionToBranch",
    "postinstall": "husky install ./config/hooks",
    "test": "mocha $(find ./lib -name '*-test.js') --require source-map-support/register",
    "util:lint": "ESLINT_STRICT=true ESLINT_EXPENSIVE=true eslint --ext .tsx --ext .ts ./src",
    "util:lint:precommit": "ESLINT_STRICT=true eslint",
    "util:validateTs": "tsc --noEmit",
    "util:validateTs:watch": "tsc --noEmit --watch --preserveWatchOutput"
  },
  "type": "module",
  "version": "1.0.0",
  "packageManager": "yarn@3.2.1",
  "resolutions": {
    "@types/node": "16.11.7"
  }
}
